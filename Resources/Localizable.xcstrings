    "help.gettingStarted.content" : {
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Speech Practice helps you rehearse speeches by reading text aloud while you follow along. Create a speech, paste or type your text, then tap Practice to begin. The app reads each segment and can pause so you can repeat it back."
          }
        }
      }
    },
    "help.playbackControls.content" : {
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "During practice, use the play/pause button to control playback. Tap the forward or backward buttons to skip between segments. You can also tap directly on any segment in the text to jump to it."
          }
        }
      }
    },
    "help.speechRate.content" : {
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Adjust the speech rate slider in Settings to control how fast text is read aloud. The range is 0.1x (very slow) to 1.0x (normal speed). Start slower when learning a new speech, then increase the speed as you become more comfortable."
          }
        }
      }
    },
    "help.pauseMode.content" : {
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Pause Mode is the core feature for speech practice. When enabled, the app pauses after reading each segment for the same duration it took to read. This gives you time to repeat the segment aloud yourself. Enable Pause Mode in Settings to practice your echo timing."
          }
        }
      }
    },
    "help.segments.content" : {
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Your speech is divided into segments for navigation and pause timing. In Settings, choose between Sentence or Paragraph granularity. Sentence mode pauses after each sentence, ideal for detailed practice. Paragraph mode pauses after each paragraph, better for practicing flow and pacing."
          }
        }
      }
    },
    "help.ttsProviders.content" : {
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "The app supports multiple text-to-speech engines."
          }
        }
      }
    },
    "help.ttsProviders.ios.content" : {
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Uses Apple's built-in speech synthesis. Works offline with no setup required. Quality varies by voice."
          }
        }
      }
    },
    "help.ttsProviders.tartuNLP.content" : {
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "A free service providing high-quality Estonian voices. Requires an internet connection. Automatically selected for Estonian text."
          }
        }
      }
    },
    "help.ttsProviders.azure.content" : {
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Premium neural voices with natural-sounding speech. Requires an Azure account and API key. Offers the highest quality voices across many languages."
          }
        }
      }
    },
    "help.voiceSelection.content" : {
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Each TTS provider offers different voices. In Settings, tap the Voice picker to choose from available voices. iOS voices marked 'Enhanced' offer better quality. For Microsoft Azure, you'll see neural voices with natural intonation. The voice list filters to show only voices that match your speech's language."
          }
        }
      }
    },
    "help.azureSetup.content" : {
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "To use Microsoft Azure voices, you need an Azure account with a Speech Services resource."
          }
        }
      }
    },
